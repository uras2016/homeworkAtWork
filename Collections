import java.util.ArrayList;

public class ArrayListExample {

  public static void main(String[] args) {
  
    // Create and populate an ArrayList
    
    ArrayList bulkRequest = new ArrayList();
    bulkRequest.add("PictureFiles");
    bulkRequest.add("MusicFiles");
    bulkRequest.add("DocFiles");
    
    
    // How many requests are there?
    int bulkRequestCount = bulkRequest.size();

    // Print the content of the ArrayList
    
    for (int i=0; i<bulkRequestCount; i++){
        System.out.println("Request #" + i + " is "+ bulkRequest.get(i));
    }
  }
}
