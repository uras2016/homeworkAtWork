import java.util.*;
 
public class CollectionApp {
 
    public static void main(String[] args) {
         
        ArrayList<String> states = new ArrayList<String>();
        states.add("Германия");
        states.add("Франция");
        states.add("Италия");
        states.add("Испания");
         
        Iterator<String> iter = states.iterator();
        while(iter.hasNext()){
         
            System.out.println(iter.next());
        }
    }
}
